{
  "name": "@lumberjack-sdk/core",
  "version": "0.7.14",
  "description": "Core JavaScript/TypeScript SDK for Lumberjack logging and tracing",
  "type": "module",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "lumberjack-sdk-cli": "./dist/cli/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "main": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "README.md",
    "dist/cli"
  ],
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "logging",
    "tracing",
    "observability",
    "typescript",
    "javascript"
  ],
  "author": "george@treebeardhq.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/treebeardhq/lumberjack-js-sdk"
  }
}
